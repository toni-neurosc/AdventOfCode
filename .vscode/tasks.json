{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Build with GCC 12.2.0",
      "command": "/home/linuxbrew/.linuxbrew/bin/g++-12",
      "args": [
        "-g",
        "-std=c++20",
        "${fileDirname}/*.cpp",
        "-o",
        "${workspaceFolder}/binaries/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build",
      "detail": "compiler: /home/linuxbrew/.linuxbrew/bin/g++-12"
    },
    {
      "type": "cppbuild",
      "label": "Build with Clang 15.0.5",
      "command": "/home/linuxbrew/.linuxbrew/bin/clang++",
      "args": [
        "-g",
        "-std=c++20",
        "${fileDirname}/*.cpp",
        "-o",
        "${workspaceFolder}/binaries/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build",
      "detail": "compiler: /home/linuxbrew/.linuxbrew/bin/clang++"
    }
  ],
  "version": "2.0.0"
}
